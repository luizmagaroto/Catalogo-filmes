generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Movie {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  title     String
  year      Int
  genre     String
  rating    Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
